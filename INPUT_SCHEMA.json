{
    "title": "Url adder",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "startUrls": {
            "title": "Search URLs",
            "type": "array",
            "description": "Search URLs",
            "prefill": [
                {
                    "url": "https://www.fiverr.com/categories/graphics-design/logo-design"
                }
            ],
            "editor": "requestListSources"
        },
        "proxyCountryCode": {
            "title": "Proxy Country Code",
            "type": "string",
